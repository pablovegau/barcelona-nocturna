---
import type { PostData } from '@/components/react/PostCard/PostCard';
import { PostListInternal } from '@/components/react/PostListInternal/PostListInternal';

interface Props {
  posts: PostData[];
  totalPages: number;
}

const { posts, totalPages } = Astro.props;

const initialData = {
  posts,
  totalPages,
};
---

<PostListInternal
  client:load 
  initialData={initialData} 
/>
